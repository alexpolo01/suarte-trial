                "summary": "Update a internalcollection by id",
                "operationId": "updateInternalCollection",
                "tags": [
                    "InternalCollections"
                ],
                "parameters": [
                    {
                        "$ref": "#/components/parameters/MongoId"
                    }
                ],
                "requestBody": {
                    "description": "InternalCollection object to be updated",
                    "required": true,
